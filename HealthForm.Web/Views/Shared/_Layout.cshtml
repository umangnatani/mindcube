<!DOCTYPE html>
<html ng-app="MyApp">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <base href="/">
    
    @*@Styles.Render("~/Content/base/baseCSS")
    @Styles.Render("~/Content/plugin/pluginCSS")
    @Styles.Render("~/Content/layout/layoutCSS")*@

    @{Html.RenderPartial("_Styles");}

    @RenderSection("styles", required: false)

</head>
<body umang-navigation-type="vertical" umang-nav-placement="left" theme-layout="wide-layout" theme-bg="bg1" ng-controller="rootController" ng-cloak>
    <div id="umangapp-wrapper" class="umang-hide-lpanel" umang-device-type="desktop">
        @{Html.RenderPartial("_Header");}
        <div id="umangapp-container" umang-color-type="lpanel-bg2" umang-lpanel-effect="shrink">
            @{Html.RenderPartial("_Menu");}

            <section id="{{MainSectionId}}">
                @*@{Html.RenderPartial("_PageTitle");}*@
                

                <div class="inner-content">
                    

                    @RenderBody()
                </div>
            </section>
            @*@{Html.RenderPartial("_SideBar");}*@
        </div>
    </div>





    @*@Scripts.Render("~/bundles/baseJS")

    @Scripts.Render("~/bundles/pluginJS")


    @Scripts.Render("~/bundles/layoutJS")

    @Scripts.Render("~/bundles/appJS"*@

    @{Html.RenderPartial("_Scripts");}

    @RenderSection("scripts", required: false)
</body>
</html>
